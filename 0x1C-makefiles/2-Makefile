# 2-Makefile
CC = gcc
NAME = school
SRC = main.c
OBJ = main.o

all: ${NAME}

${NAME}: ${OBJ} ${OBJ:main.o=school.o}
	${CC} ${OBJ}  ${OBJ:main.o=school.o} -o ${NAME}

${OBJ} : ${SRC}
	${CC} -c -o ${OBJ} ${SRC}

${OBJ:main.o=school.o} : ${SRC:main.c=school.c}
	${CC} -c -o ${OBJ:main.o=school.o} ${SRC:main.c=school.c}
